  <body>
    <tu>
      <tuv xml:lang="smj">
        <seg>Åhpav: Biejvve</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Lïerh: Biejjie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Assisia Fránsa Biejvvelávlla</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Mov Åejvie, datnem heevehtem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Guddnedum lehkus dån, Hærrám aktan gájkka majt sjivnnjedam le</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>gaajhkine ektesne maam datne sjugniehtamme,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>sierraláhkáj viellja Biejvve, mij midjij biejvev vaddá,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>mij aeredslååjjedem mijjese buakta,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>ja dån midjij tjuovgav dan baktu vattá.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>jïh numhtie datne tjoevkesem mijjese vadtah.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Guddnedum lehkus dån, Hærrám,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Mov Åejvie, datnem heevehtem,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Biegga-vielja åvdås ja ilme ja balvaj åvdås</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>voejngenassen jïh balvi,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>ja alme ja juohkkalágásj dálke åvdås,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>dovne elmien jïh ovmessie vearoldi åvteste,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>danen gå dan baktu dån dibdá gájkka</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>juktie dejgujmie datne gaajhkem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>ietjat sjivnnjádusáv bierggit.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>maam sjugniehtamme, jielemem vadtah.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Guddnedum lehkus dån, Hærrám,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Mov Åejvie, datnem heevehtem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ednam-oappá åvdås, mijá ieddne,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>mijjen Åabpa Eatnemen åvteste</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>mij mijáv baddjen anet ja guoddá</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>mij mijjem bïpmehte jïh vaarjele</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>ja muorjijt ja rásev vaddá</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>muerjieh, kraesieh jïh sjædtoeh</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>ja bájnos sjattojt.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>ovmessie klaerine buakta.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Assisia Fránssa riegádij jagen1182 Umbrian Italian.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Assissijen Franse jaepien 1182 Umbrijisnie, Italijisnie reakasovvi.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Biednikniehkke Fránssa vattij gájkka majt åmastij.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Franse ræjhkoes ålman baernie, dïhte gaajhkem eekem jeatjabidie vedtieji.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ietjas iellemav vattij råhkådallamij ja hæjojt dievnnutjit: Jubmela sjivnnjádusáv guddnedij ietjas iellema baktu.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Dan mænngan sïjhti rohkelidh jïh giefies almetjidie vïehkiehtidh jïh Jupmelen sjugniehtimmiem heevehtidh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ietjas dåbdojt ilmodij tjáppa diktaj ja girjij baktu.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Guktie Franse ussjedi, tjaebpies tjïhteginie jïh prïevine tjeeli.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Lågå ienep suv birra</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Lohkh vielie Fransen bïjre deasnie</seg>
      </tuv>
    </tu>
  </body>

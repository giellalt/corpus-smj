<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="smj" datatype="plaintext">
    <prop type="x-filename">index.php_kat_id=151_art_id=164.html.xml</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="smj">
        <seg>Åhpav: Låvdagoahte</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Lïerh: Låavth-gåetie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Áhtje mijá</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aehtjie mijjen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Áhttje mijá guhti le almen.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aehtjie mijjen guhte leah Elmie-rijhkesne.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ájlistuvvus duv namma.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Baajh dov nommem aejliestovvedh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Båhtus duv rijkka.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Baajh dov rijhkem båetedh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Sjaddus duv sidot</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Baajh dov syjhtedem eatnamisnie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>gåk almen, nåv aj ednamin.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>sjidtedh guktie Elmie-rijhkesne.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Vatte midjij uddni mijá bæjvvásasj lájbev.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Vedtieh mijjese daan biejjien mijjen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ja luojte midjij suttojdimme ándagis,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>fierhten-beajjetje laejpiem.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>nåv gåk mij aj luojttep mijá velgulattjajda.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Luejhtieh mijjeste maam mijjieh meadteme guktie mijjieh luejhtebe dejstie guhth mijjese meadteme.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ja ale mijáv gæhttjalibmáj lájddi,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aellieh luejhtieh mijjem giehtjelimmij sijse,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>ájnat várjjala mijáv bahás.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>valla vaarjelh mijjem bahheste.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Juhte duv le rijkka ja fábmo</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Juktie rijhke lea dov, faamoe jih earoe ihkuven aajkan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>ja herlukvuohta ihkeven ájggáj.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aamen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Jesus mijájt åhpadij Áhtje mijáv råhkådallat.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jeesuse Aehtjie mijjen mijjem lieri.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Danna li gietjav råhkusa, ja dá gietjav råhkusa gåbttji dav man åvdås dárbahip råhkåt.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Dan lea tjïjhtje rohkelassh, jïh dah tjïjhtje rohkelassh voebnesjieh gaajhkem maam mijjieh daarpejibie rohkelassese.</seg>
      </tuv>
    </tu>
  </body>

    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Sáme girkkoráde ásaduváj Girkkotjåhkanimes jagen 1992.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Gærhkoetjåanghkoe Saemien gærhkoeraeriem tseegki jaepien 1992.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ráde galggá åvdedit, suoddjit ja aktidit sáme girkkoiellemav Vuona girkkon.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Raerie edtja saemien gærhkoejielemem Dennie nøørjen gærhkosne evtiedidh, vaarjelidh jïh iktedidh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Sáme girkkoráde bargguj gullu gájkka ássje ma ráde milta sáme girkkoiellemij gulluji.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Saemien gærhkoeraerien barkoesuerkie lea gaajhkh aamhtesh mah raerien mielen mietie saemien gærhkoejielemem doehtedieh</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Da gålmmå stuorra sáme álmmukjuohkusa Vuonan, nuorttasáme, julevsáme ja oarjjelsáme, li ájrrasa ráden manna li gáktsa ájrrasa.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Dah golme stoerre saemien almetjetjierth Nøørjesne, noerhtesaemien, julevsaemien jïh åarjelsaemieh, leah meatan raeresne, gusnie gaektsie lïhtsegh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Sáme girkkoráde ájrrasa li</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Saemien gærhkoeraerien lïhtsegh leah:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Anne Dalheim (jådediddje),</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>åvtehke</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Ingar Nikolaisen Kuoljok, julevsáme ájras Oarjje-Hålogaláttes,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>julevsaemien tjirkije / Åarjel-Hålogalaanten bispedajve,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Jonhild Joma, oarjjelsáme ájras Nidarosas</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jonhild Joma,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Kirsten Isaksen, nuorttasáme ájras Nuortta-Hålogaláttes</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Kirsten Isaksen, noerhtesaemin tjirkije / Noerhte-Hålogalaanten bispedajv</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Tor Berger Jørgensen, Duodden Bisspatjåhkanime ájras</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>saemien tjirkije Åarjel-Nøørjeste,</seg>
      </tuv>
    </tu>

  <body>
    <tu>
      <tuv xml:lang="smj">
        <seg>Lága ja njuolgadustjállaga / Giella / Sámedigge - Sametinget</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Laakh / Healsoe jïh sosijaale / Saemiedigkie - Sametinget</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Sválldasa lágajda ja njuolgadustjállaga ma gulluji sámegiellaj</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Svaaltesh laakide mah saemien pasieenti jïh utnjiji reaktah dijpieh</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Politihkalasj åvdåsvásstediddje</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Politihkere gïen diedte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>(Dåssju dárruj)</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>NSR</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Njuolgadustjállaga ma guosski Sámeláhkaj:</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Phone: +47 907 75 219</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Njuolgadustjála sámeláhkaj (láhka biehtsemánno 12.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Laake Saemiedigkien jïh jeatjah saemien reaktatsiehkiej bïjre (saemielaake).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>B 1987 nr 56) sámegielaj háldadusguovllo lága birra</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>(barre daaroen) § 3-5.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Njuolgadustjála sámelága giellarievtesvuoda birra</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Vijriedamme reakta saemien healsoe- jïh sosiaalesektovrem utnedh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Njuolgadustjála sámegiela åhpadime rievtesvuoda birra</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Laake tjïelten healso- jïh hoksehtimmiedïenesji j. j bïjre (healsoe- jïh hoksehtimmiedïenesjelaake).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Bájkkenammaláhka:</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>(barre daaroen) § 3-10.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Láhka bájkkenamáj birra</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Pasieenti jïh utniji tsevtsemen bïjre jïh laavenjostedh jïjtsh mielen miete organisasjovnigujmie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Njuolgadustjála bájkkenamáj birra</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Laake pasieenti- jïh utnijereaktaj bïjre (pasieente- jïh utnijereaktalaake).</seg>
      </tuv>
    </tu>

    <tu>
      <tuv xml:lang="smj">
        <seg>Giehtabádde, boallomærkka ja báhkogirjásj nåvkå / Sámásta mujna / Giella / Sámedigge - Sametinget</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Åadtjoeh namhtah gïetebaantem, båaloem jïh baakoegærjetjem / Saemesth munnjien / Gïele / Saemiedigkie - Sametinget</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Rávvimsjiebmá</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Dongkemegoere</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Vállji ådå</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Veeljh dorjesem</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Duodasta rávvagav</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Nomme jïh adresse</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="smj">
        <seg>Gijtto rávvagis</seg>
      </tuv>
      <tuv xml:lang="sma">
